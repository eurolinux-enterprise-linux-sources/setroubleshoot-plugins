#: ../src/connect_ports.py:39
#, python-format
msgid ""
"\n"
"    If you want to allow $SOURCE to connect to $PORT_NUMBER, you can execute \n"
"\n"
"    # semanage port -a -t PORT_TYPE -p %s $PORT_NUMBER\n"
"\n"
"    where PORT_TYPE is one of the following: %s.\n"
"    "
msgstr "\n    Aby pozwolić $SOURCE na łączenie się z $PORT_NUMBER, można wykonać\n\n    # semanage port -a -t TYP_PORTU -p %s $PORT_NUMBER,\n\n    gdzie TYP_PORTU jest jednym z: %s.\n    "

#: ../src/cvs_data.py:27
